.container{
  padding: 0 84rpx;
  font-size: 30rpx;
  color: #333333;
  overflow: hidden;
  width: 100%;
  box-sizing: border-box;
}
.list{
  width: 100%;
  margin-top:16rpx;
  height: 130rpx;
  line-height: 130rpx;
  position: relative;
}
image{
  width: 14rpx;
  height: 24rpx;
  position: absolute;
  right: 4rpx;
  top:50%;
  transform: translate(0,-50%);
}
.line{
  border-bottom:2rpx solid #999999;
}